    @RenderSection("Scripts", required: false)

    <script>
        $(document).ready(function () {
            var pgurl = window.location.pathname;




            $("nav ul a").each(function () {


                if ($(this).attr("href") == pgurl || $(this).attr("href") == '') {
                    $(this).addClass("active");

                    var childId = $(this).attr("id");

                    var someElement = document.getElementById("dropparntactive_" + childId.replace("child_", ""));
                    someElement.className += " active";

                    var someElement = document.getElementById("dropdownParrent_" + childId.replace("child_", ""));
                    someElement.className += " menu-open";

                }
                /*document.getElementById("2").addClass(' active');*/


            })
        });
    </script>
